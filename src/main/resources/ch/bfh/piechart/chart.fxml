<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="ch.bfh.piechart.ui.Controller"
        styleClass="pane"
        alignment="CENTER">
    <stylesheets>
        <URL value="@chart.css"/>
    </stylesheets>

    <Label fx:id="heading" styleClass="error" text="The SalesValue PieChart"/>

    <Pane fx:id="pane" minHeight="30.0" minWidth="30.0" prefHeight="600.0" prefWidth="600.0"/>

</VBox>
